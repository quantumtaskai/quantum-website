{
  "name": "quantumtaskai-website-test",
  "type": "compose",
  "description": "Quantum Tasks AI website - Testing without domain",
  "version": "1.0.0",
  "compose": {
    "file": "docker-compose.test.yml",
    "context": "."
  },
  "environment": {
    "NODE_ENV": "production"
  },
  "resources": {
    "memory": "256M",
    "cpu": "0.5"
  },
  "healthcheck": {
    "enabled": true,
    "path": "/",
    "interval": 30,
    "timeout": 10,
    "retries": 3
  },
  "ports": [
    {
      "containerPort": 80,
      "hostPort": 8080,
      "protocol": "tcp"
    }
  ]
}
